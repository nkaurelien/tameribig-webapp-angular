{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@lib/*": [
        "src/@lib/*"
      ],
      "@core/*": [
        "src/@core/*"
      ],
      "@data/*": [
        "src/@data/*"
      ],
      "@modules/*": [
        "src/app/*"
      ],
      "@app/*": [
        "src/app/*"
      ],
      "@environments/*": [
        "src/environments/*"
      ],
      "@services/*": [
        "src/@services/*"
      ],
      "@schemas": [
        "src/@schemas"
      ],
      "@@/*": [
        "src/*"
      ],
      "~src/*": [
        "src/*"
      ]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts"
  ]
}
